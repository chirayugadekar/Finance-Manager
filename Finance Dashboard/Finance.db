import sqlite3

# Connect to SQLite database (will create if it doesn't exist)
conn = sqlite3.connect("finance.db")
c = conn.cursor()

# Create a table
c.execute('''
CREATE TABLE IF NOT EXISTS transactions (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    date TEXT,
    expenses REAL,
    category TEXT,
    type TEXT,
    description TEXT
)
''')

conn.commit()
conn.close()
